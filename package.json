{
  "name": "destroyable-bucket",
  "version": "1.13.1",
  "description": "Construct that allows the CDK to destroy a non-empty bucket when employing the `cdk destroy` command. This is accomplised by emptying the bucket before destroying it.",
  "main": "dist/src/resource/destroyable-bucket.js",
  "types": "dist/src/resource/destroyable-bucket.d.ts",
  "keywords": [
    "aws",
    "s3",
    "cloudformation",
    "bucket",
    "delete",
    "destroy",
    "cdk",
    "construct",
    "non-empty"
  ],
  "homepage": "https://github.com/NightWatchman/destroyable-bucket",
  "scripts": {
    "test": "jest --silent",
    "local:create": "npm webpack && lambda-local -l dist/src/lambda/main.js -e test/fixtures/create.json",
    "local:delete": "npm webpack && lambda-local -l dist/src/lambda/main.js -e test/fixtures/delete.json",
    "bump:patch": "npm version patch -m 'Bump version'",
    "bump:minor": "npm version minor -m 'Bump version'",
    "bump:major": "npm version major -m 'Bump version'",
    "build": "tsc && webpack",
    "cdk": "npm compile && cdk",
    "cdk:list": "npm compile && cdk list",
    "cdk:synth": "npm compile && cdk synth",
    "cdk:deploy": "npm compile && cdk deploy",
    "cdk:destroy": "npm compile && cdk destroy",
    "cdk:version": "npm compile && cdk --version"
  },
  "author": "Eric Rushing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NightWatchman/destroyable-bucket"
  },
  "dependencies": {
    "aws-sdk": "^2.550.0",
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@aws-cdk/aws-cloudformation": "^1.13.0",
    "@aws-cdk/aws-lambda": "^1.13.0",
    "@aws-cdk/aws-s3": "^1.13.0",
    "@aws-cdk/core": "^1.13.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.12.2",
    "@types/uuid": "^3.4.4",
    "aws-cdk": "^1.13.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "jest": "^24.6.0",
    "lambda-local": "^1.5.2",
    "node-fixtures": "^0.0.1",
    "typescript": "^3.4.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "jest"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "semi": [
        2,
        "never"
      ]
    }
  }
}
